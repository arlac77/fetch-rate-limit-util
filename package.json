{
  "name": "fetch-rate-limit-util",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "packageManager": "npm@11.10.0",
  "types": "./types/rate-limit-util.d.mts",
  "exports": {
    ".": {
      "types": "./types/rate-limit-util.d.mts",
      "default": "./src/rate-limit-util.mjs"
    }
  },
  "description": "handle fetch errors and rate limits",
  "keywords": [
    "etag",
    "fetch",
    "rate-limit"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "0BSD",
  "scripts": {
    "prepare": "node --run prepare:typescript",
    "prepare:typescript": "tsc --allowJs --declaration --emitDeclarationOnly --declarationDir types --resolveJsonModule --target esnext -m esnext --module nodenext --moduleResolution nodenext --rootDir src ./src**/*.mjs",
    "test": "node --run test:browser-ava && node --run test:ava",
    "test:ava": "ava --timeout 20m tests/*-ava.mjs tests/*-ava-node.mjs",
    "test:browser-ava": "browser-ava --headless --no-keep-open tests/*-ava.mjs tests/*-ava-browser.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 20m tests/*-ava.mjs tests/*-ava-node.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme --section=API ./src**/*.mjs",
    "lint": "node --run lint:docs && node --run lint:typescript",
    "lint:docs": "documentation lint ./src**/*.mjs",
    "lint:typescript": "tsc --allowJs --checkJs --noEmit --resolveJsonModule --target esnext -m esnext --module nodenext --moduleResolution nodenext ./src**/*.mjs"
  },
  "devDependencies": {
    "ava": "^7.0.0",
    "browser-ava": "^2.3.54",
    "c8": "^11.0.0",
    "documentation": "^14.0.3",
    "semantic-release": "^25.0.3",
    "typescript": "^5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/fetch-rate-limit-util.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/fetch-rate-limit-util/issues"
  },
  "homepage": "https://github.com/arlac77/fetch-rate-limit-util#readme",
  "template": {
    "properties": {
      "ava": {
        "timeout": "20m"
      }
    },
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-browser-ava",
      "arlac77/template-javascript-component",
      "arlac77/template-typescript"
    ]
  }
}
